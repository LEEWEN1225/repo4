<button class="btn btn-primary" onclick="getEmployees()">getEmployees</button>


@section Scripts{
    <script>

        var baseAddress = "https://localhost:7241";

        function getEmployees() {
            //alert("getEmployees")
            $.ajax({
                type: "GET",
                url: `${baseAddress}/api/Employees`


            }).done(function (data) {
                alert(data)

            }).fail(function (err) {
                alert(`${err.statusCode}`);

            });
        }
    </script>
}